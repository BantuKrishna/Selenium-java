package pageObjects;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class CalendarPage extends BasePage {
	
	public CalendarPage(WebDriver driver) {
		super(driver);
	}
	
	@FindBy(xpath="//*[@id=\"root\"]/div/div[1]/div[2]/div[2]/div[1]/ul/li[2]/div/div")
	public WebElement cal;
	
	@FindBy(xpath="//*[@id=\"root\"]/div/div[1]/div[2]/div[3]/div/div/div/div/div[1]/div/div[1]/div/span[3]")
	public  WebElement next;
	
	@FindBy(xpath="//*[@id=\"root\"]/div/div[1]/div[2]/div[3]/div/div/div/div/div[1]/div/div[2]/span")
	public WebElement room;
	
	@FindBy(xpath="/html/body/div[2]/div[3]/ul/div[2]/div[1]/div[2]/span")
	public WebElement filter;
	
	@FindBy(xpath="/html/body/div[2]/div[3]/ul/div[3]/div[2]/button")
	public  WebElement app;
	
	@FindBy(name="meetName")
	public WebElement mname;
	
	@FindBy(name="meetDescription")
	public WebElement mdesc;
	
	@FindBy(xpath="//*[@id=\"root\"]/div/div[1]/div[2]/div[3]/div/div/div/div/div/div[2]/div[4]/div[1]/div")
	public WebElement date;
	
	@FindBy(xpath="//*[@id=\"basic-menu\"]/div[3]/ul/div/div/div/div[2]/div/div/div[2]/div/div[4]/button[7]")
	public WebElement date1;
	
	@FindBy(xpath="//*[@id=\"root\"]/div/div[1]/div[2]/div[3]/div/div/div/div/div/div[2]/div[4]/div[2]/div")
	public WebElement time;
	
	@FindBy(xpath="//li[@value='62']")
	public WebElement  time1;
	
	@FindBy(xpath="//*[@id=\"root\"]/div/div[1]/div[2]/div[3]/div/div/div/div/div/div[2]/div[4]/div[3]/div")
	public WebElement time2;
	
	@FindBy(xpath="//*[@id=\"basic-menu\"]/div[3]/ul/li[65]")
	public WebElement time3;
	
	

}
