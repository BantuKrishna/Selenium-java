@Test
	
	public void EditMeetingPageTC_01() throws Exception {
	
	EditMeetingPage Mp=new EditMeetingPage (driver);
	Thread.sleep(3000);
	JavascriptExecutor js=  (JavascriptExecutor)driver;
	js.executeScript("arguments[0].scrollIntoView;", Mp.Meet);
			
	Mp.Meet.click();
	Thread.sleep(3000);
	Mp.Edit.click();
	Thread.sleep(3000);
	Mp.Title.sendKeys("and Dev Team");
	Thread.sleep(3000);
	
	
	
	

}
}
