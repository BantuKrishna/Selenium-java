package pageObjects;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class CalendarPage extends BasePage {
	
	public CalendarPage(WebDriver driver) {
		super(driver);
	}
	
	@FindBy(xpath="//*[@id=\"root\"]/div/div[1]/div[2]/div[2]/div[1]/ul/li[2]/div/div")
	public WebElement cal;
	
	@FindBy(xpath="//*[@id=\"root\"]/div/div[1]/div[2]/div[3]/div/div/div/div/div[1]/div/div[1]/div/span[3]")
	public  WebElement next;
	
	@FindBy(xpath="//*[@id=\"root\"]/div/div[1]/div[2]/div[3]/div/div/div/div/div[1]/div/div[2]/span")
	public WebElement room;
	
	@FindBy(xpath="/html/body/div[2]/div[3]/ul/div[2]/div[1]/div[2]")
	public WebElement filter;
	

}
